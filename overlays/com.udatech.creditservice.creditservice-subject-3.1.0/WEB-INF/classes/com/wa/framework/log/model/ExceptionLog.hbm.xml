<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.wa.framework.log.model">
	<class
		name="ExceptionLog"
		table="EXCEPTION_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="LOG_ID"
		>
			<generator class="uuid"/>
		</id>

		<property
			name="accessUrl"
			column="ACCESSURL"
			type="string"
		/>
		<property
			name="methodName"
			column="METHODNAME"
			type="string"
		/>
		<property
			name="username"
			column="USERNAME"
			type="string"
		/>
		<property
			name="ip"
			column="IP"
			type="string"
		/>
		<property
			name="logParams"
			column="LOGPARAMS"
			type="string"
		/>
		<property
			name="logDate"
			column="LOGDATE"
			type="timestamp"
		/>
		<property
			name="type"
			column="TYPE"
			type="string"
		/>
		<property
			name="departname"
			column="DEPARTNAME"
			type="string"
		/>
		<property
			name="userId"
			column="USERID"
			type="string"
		/>
		<property
			name="message"
			column="MESSAGE"
			type="string"
		/>
		

	</class>	
</hibernate-mapping>