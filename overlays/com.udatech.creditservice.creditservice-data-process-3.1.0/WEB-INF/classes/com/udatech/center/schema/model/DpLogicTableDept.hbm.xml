<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.udatech.center.schema.model.DpLogicTableDept" table="DP_LOGIC_TABLE_DEPT">

        <id name="id" type="java.lang.String">
            <column name="ID" length="50"/>
            <generator class="uuid.hex"/>
        </id>

        <property name="logicTableId" type="java.lang.String">
            <column name="LOGIC_TABLE_ID"/>
        </property>
        <property name="deptId" type="java.lang.String">
            <column name="DEPT_ID"/>
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="CREATE_USER"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME"/>
        </property>
        <property name="versionId" type="java.lang.String">
            <column name="TABLE_VERSION_ID"/>
        </property>
        <property name="versionConfigId" type="java.lang.String">
            <column name="TABLE_VERSION_CONFIG_ID"/>
        </property>
    </class>
</hibernate-mapping>
