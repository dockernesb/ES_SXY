<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.udatech.center.schema.model.DpLogicColumn"
		table="DP_LOGIC_COLUMN">

		<id name="id" type="java.lang.String">
			<column name="ID" length="50" />
			<generator class="uuid.hex" />
		</id>

		<property name="logicTableId" type="java.lang.String">
			<column name="LOGIC_TABLE_ID" />
		</property>
		<property name="code" type="java.lang.String">
			<column name="CODE" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="TYPE" />
		</property>
		<property name="len" type="java.lang.Integer">
			<column name="LENGTH" />
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="STATUS" />
		</property>
		<property name="mappedCode" type="java.lang.String">
			<column name="MAPPED_CODE" />
		</property>
		<property name="isCore" type="java.lang.Integer">
			<column name="IS_CORE" />
		</property>
		<property name="isNullable" type="java.lang.Integer">
			<column name="IS_NULLABLE" />
		</property>
		<property name="isRepeat" type="java.lang.Integer">
			<column name="IS_REPEAT" />
		</property>
		<property name="isUpdate" type="java.lang.Integer">
			<column name="IS_UPDATE" />
		</property>
		<property name="isSearch" type="java.lang.Integer">
			<column name="IS_SEARCH" />
		</property>
		<property name="requiredGroup" type="java.lang.String">
			<column name="REQUIRED_GROUP" />
		</property>
		<property name="postil" type="java.lang.String">
			<column name="POSTIL" />
		</property>
		<property name="fieldSort" type="java.lang.Integer">
            <column name="FIELD_SORT" />
        </property>
		<property name="commonFieldId" type="java.lang.String">
			<column name="COMMON_FIELD_ID" />
		</property>
	</class>
</hibernate-mapping>
