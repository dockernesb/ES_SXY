<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.udatech.common.resourceManage.model">
	<class name="Watermark" table="DZ_CREDIT_WATERMARK">
		<meta attribute="sync-DAO">false</meta>
		<id column="ID" name="id" type="string">
			<generator class="uuid" />
		</id>
		<property column="NAME" name="name" />
		<property column="FILE_NAME" name="fileName" />
		<property column="CREATE_USER_NAME" name="createUseName" />
		<property column="CREATE_TIME" name="createTime" />
	</class>
</hibernate-mapping>
